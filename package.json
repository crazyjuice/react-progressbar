{
  "name": "cjs-react-progressbar",
  "appName": "ProgressBar",
  "version": "1.2.4",
  "description": "React Progress Bar",
  "main": "./lib/ProgressBar.min.js",
  "repository": "git@github.com:crazyjuice/react-progressbar.git",
  "author": "Piotr Sikora",
  "keywords": [
    "progress",
    "progress bar",
    "react",
    "react progress bar",
    "top progress bar",
    "custom progress bar"
  ],
  "license": "MIT",
  "scripts": {
    "test": "mocha \"src/**/*.test.js*\" -r babel-core/register -r ignore-styles -r setupTests.js",
    "test:cover": "nyc npm test",
    "test:watch": "npm run test -- --watch",
    "coveralls": "npm run test:cover && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --ext .js,.jsx src && npm run lint:post",
    "lint:fix": "eslint --fix --ext .jsx,.js src && npm run lint:post",
    "lint:post": "echo \"No ESLint errors!!!\"",
    "clean": "rimraf dist lib package .nyc_output coverage *.tgz webpack-stats* yarn-error*",
    "build": "npm run lint && cross-env NODE_ENV=production webpack --color --progress --config webpack.config.prod.js --json > webpack-stats.json",
    "lib": "cross-env NODE_ENV=production webpack --color --progress --config webpack.config.lib.js",
    "build:lib": "npm run lint && rimraf lib && npm run lib -- --env.MINIFY=1 -p && npm run lib -- --env.MINIFY=0",
    "server": "node index.js",
    "server:dev": "node server.js",
    "serve": "webpack-dev-server --open --color --progress --config webpack.config.dev.js --env.MIDDLEWARE=0",
    "start": "npm run lint && npm run server:dev",
    "start:dev": "npm run lint && npm run serve"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.6.2",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "nyc": "^11.4.1",
    "postcss-loader": "^2.0.9",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {}
}
